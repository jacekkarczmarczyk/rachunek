{
  "name": "rachunek",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite --port=9001 --open",
    "build": "pnpm run type-check && pnpm run build-only",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --fix",
    "test": "pnpm run type-check && pnpm run lint",
    "preview": "vite preview --port=9001 --open"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@vueuse/core": "^10.7.1",
    "axios": "^1.6.3",
    "roboto-fontface": "^0.10.0",
    "sass": "^1.69.7",
    "slownie": "^1.0.1",
    "systemjs": "^6.14.3",
    "vue": "^3.4.3",
    "vue-loader": "^17.4.2",
    "vue-router": "^4.2.5",
    "vuetify": "^3.4.9",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@vitejs/plugin-vue": "^5.0.2",
    "@vue/runtime-dom": "^3.4.3",
    "eslint": "^8.56.0",
    "eslint-config-jkarczm": "^10.0.0",
    "importmap-plugin": "^0.8.0",
    "typescript": "^5.3.3",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.10",
    "vite-plugin-checker": "^0.6.2",
    "vue-tsc": "^1.8.27"
  }
}
