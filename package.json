{
  "name": "rachunek",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite --port=9001 --open",
    "build": "pnpm run type-check && pnpm run build-only",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --fix",
    "test": "pnpm run type-check && pnpm run lint",
    "preview": "vite preview --port=9001 --open"
  },
  "dependencies": {
    "@mdi/font": "^7.0.96",
    "@vueuse/core": "^9.5.0",
    "axios": "^1.1.3",
    "roboto-fontface": "*",
    "sass": "^1.56.1",
    "slownie": "^1.0.1",
    "systemjs": "^6.13.0",
    "vue": "^3.2.45",
    "vue-loader": "^17.0.1",
    "vue-router": "^4.1.6",
    "vuetify": "3.0.0",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/runtime-dom": "^3.2.45",
    "eslint": "^8.27.0",
    "eslint-config-jkarczm": "^10.0.0-beta.1",
    "importmap-plugin": "^0.6.1",
    "typescript": "^4.8.4",
    "unplugin-vue-components": "0.22.7",
    "vite": "^4.0.0-alpha.1",
    "vite-plugin-checker": "^0.5.1",
    "vue-tsc": "^1.0.9"
  }
}
