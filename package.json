{
  "name": "rachunek",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite --port=9001 --open",
    "build": "pnpm run type-check && pnpm run build-only",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --fix",
    "test": "pnpm run type-check && pnpm run lint",
    "preview": "vite preview --port=9001 --open"
  },
  "dependencies": {
    "@mdi/font": "^7.1.96",
    "@vueuse/core": "^9.13.0",
    "axios": "^1.3.4",
    "roboto-fontface": "^0.10.0",
    "sass": "^1.59.3",
    "slownie": "^1.0.1",
    "systemjs": "^6.14.0",
    "vue": "^3.2.47",
    "vue-loader": "^17.0.1",
    "vue-router": "^4.1.6",
    "vuetify": "^3.1.10",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.3",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/runtime-dom": "^3.2.47",
    "eslint": "^8.36.0",
    "eslint-config-jkarczm": "10.0.0-beta.4",
    "importmap-plugin": "^0.8.0",
    "typescript": "^4.9.5",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.1.4",
    "vite-plugin-checker": "^0.5.6",
    "vue-tsc": "^1.2.0"
  }
}
